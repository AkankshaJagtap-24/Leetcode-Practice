class Solution {
    public int reverse(int x) {
        //Scanner sc = new Scanner(System.in);
        int rev = 0;
        //int x = sc.nextInt();
        int original = x;
        while (x != 0){
            int digit  = x % 10;
            rev = rev * 10 + digit;
            x = x /10;
        }
        //System.out.println( "reverse of"  + original + " " + rev);
        return rev;

        
        
    }
}
